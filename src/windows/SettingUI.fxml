<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<BorderPane prefHeight="500.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/10" xmlns:fx="http://javafx.com/fxml/1" fx:controller="windows.SettingController">
         
         <bottom>
         	<VBox alignment="CENTER" prefHeight="200.0" prefWidth="300.0">
         	
	         	<children>		
	  	
		
	     		<Button alignment="CENTER" defaultButton="true" mnemonicParsing="false" onAction="#buttonMain" text="Main Menu" textAlignment="CENTER" style="-fx-font-size: 25; -fx-text-fill: #440235; -fx-padding: 0; -fx-pref-width: 350; -fx-border-color: blue;" />
	
	 			</children>
	 		</VBox>
         </bottom>
   <center>
      <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
         <children>
            <Label text="Choose dimension of game board" />
            <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
               <children>
                  <Button fx:id="five" mnemonicParsing="false" text="5" onAction="#buttonFive"/>
                  <Button fx:id="ten" mnemonicParsing="false" text="10" onAction="#buttonTen"/>
               </children>
            </HBox>
         </children>
      </VBox>
   </center>
</BorderPane>
